// Demonstrate immutable and mutable variables

// Immutable variables (let)
let immutable_x = 10;
output(immutable_x);

// Mutable variables (let var)
let var mutable_y = 20;
output(mutable_y);

// Can reassign mutable variables
mutable_y := 30;
output(mutable_y);

// Can use immutable variables in expressions
let sum = immutable_x + mutable_y;
output(sum);

// Mutable with type annotation
let var count: Int = 0;
count := count + 1;
count := count + 1;
output(count);

// String mutability
let var message = 'Hello';
message := 'World';
output(message);

// List mutability
let var numbers = [1, 2, 3];
output(len(numbers));
numbers := [4, 5, 6, 7, 8];
output(len(numbers))
