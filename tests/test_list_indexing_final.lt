output(nums[2]);
output(nums[4]);

// Test with variable index
let i = 2;
output(nums[i]);

// Test with expression as index
output(nums[1 + 1]);
output(nums[i - 1]);

// Test with different types
let strings = ['hello', 'world', 'lift'];
